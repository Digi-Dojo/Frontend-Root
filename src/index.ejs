        "@startup-digi-dojo-lab/task-create-form": "<%= isLocal ? '//localhost:8080/startup-digi-dojo-task-create-form.js' : 'https://sddl-task-create-form-r21m.onrender.com/startup-digi-dojo-task-create-form.js' %>",
        "@startup-digi-dojo-lab/task-list": "<%= isLocal ? '//localhost:8081/startup-digi-dojo-lab-task-list.js' : 'https://sddl-task-list-t9e4.onrender.com/startup-digi-dojo-lab-task-list.js' %>",
      
        "@startup-digi-dojo-lab/note-creator": "<%= isLocal ? 'https://sddl-note-creator.onrender.com/startup-digi-dojo-lab-note-creator.js' : 'https://sddl-note-creator.onrender.com/startup-digi-dojo-lab-note-creator.js' %>",
        "@startup-digi-dojo-lab/place-creator": "<%= isLocal ? 'https://sddl-place-creator.onrender.com/startup-digi-dojo-lab-place-creator.js' : 'https://sddl-place-creator.onrender.com/startup-digi-dojo-lab-place-creator.js' %>",
